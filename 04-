#include <bits/stdc++.h>
using namespace std;

int main() {
    int n;
    cin >> n;

    float vetor[n], soma = 0;

    for (int i = 0; i < n; i++) {
        cin >> vetor[i];
        soma += vetor[i];
    }

    float media = soma / n;
    cout << fixed << setprecision(3) << media << endl;

    for (int i = 0; i < n; i++) {
        if (vetor[i] < media) {
            cout << fixed << setprecision(1) << vetor[i] << endl;
        }
    }

    return 0;
}
