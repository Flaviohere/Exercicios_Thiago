#include <bits/stdc++.h>
using namespace std;

int main() {
    int n;
    cin >> n;

    string nome[n];
    float nota[n];
    float nota2[n];
    int aprovado = 0;

    printf("Alunos aprovados:\n");
    for (int i = 0; i < n; i++) {
        cin >> nome[i] >> nota[i] >> nota2[i];
        if ((nota[i] + nota2[i]) / 2 >= 6.0) {
            printf("%s\n", nome[i].c_str());
        }
    }  
}
