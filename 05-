#include <bits/stdc++.h>
using namespace std;

int main() {
    int n;
    cin >> n;

    int vetor[n];
    int media = 0;
    int pares = 0;

    for (int i = 0; i < n; i++) {
        cin >> vetor[i];
        if (vetor[i] % 2 == 0) {
        media += vetor[i];
        pares ++;
        }
     }
    
    media = media / pares;
    printf("media: %d\n", media);
}
